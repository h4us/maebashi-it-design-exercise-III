{
    "name": "editor-project-sample",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "clean": "rimraf build declaration dist out",
        "compile": "tsc -p .",
        "build": "webpack --mode production",
        "out": "run-script-os",
        "wincp": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./cp.ps1",
        "out:win32": "webpack --mode production && tailwindcss -i ./styles/main.css -o ./dist/main.css && npm run wincp",
        "out:default": "webpack --mode production && tailwindcss -i ./styles/main.css -o ./dist/main.css && mkdir -p out && cp -rf index.html dist scenes _streamingAssets out",
        "watch": "webpack --mode development --watch",
        "webserver": "http-server -p 1338 -c-1",
        "style-dev": "tailwindcss -i ./styles/main.css -o ./dist/main.css --watch"
    },
    "license": "(Apache-2.0)",
